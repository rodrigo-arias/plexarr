# UID and GID of the user running Docker
PUID=1000
PGID=1000

# Time zone
TZ=America/Argentina/Buenos_Aires

# qBittorrent
QBIT_WEBUI_USERNAME=admin
# Leave empty on first run to generate a temporary password in logs
QBIT_WEBUI_PASSWORD=

# Pi-hole
PIHOLE_WEBPASSWORD=changeme

# Optional: fixed IP of the NAS if your router requires it (can be left empty)
